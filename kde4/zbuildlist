for f in $FILES
do
  if [ ! -e /tmp/$f.built ]
  then
    ./$f.build -j5

#  echo "now install $f"
#  read junk

    su -c "upgradepkg --install-new --reinstall /tmp/packages/$f*"

    touch /tmp/$f.built
  fi

done

echo alldone

