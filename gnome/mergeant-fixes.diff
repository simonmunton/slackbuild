+++ mergeant-0.62/DevGuide/picts/Makefile	2002-08-18 18:32:12.000000000 -0400
@@ -0,0 +1,37 @@
+# images files
+dia_images := ConfManager.dia queries.dia Query.dia QueryEditor.dia QueryField.dia QueryJoin.dia QueryJoin_sample.dia
+png_images := QueryEditorSC.png QueryEditorSC2.png
+
+all_picts: eps_picts png_picts
+
+eps_picts: dia_eps_files dia_eps_link_files png_eps_files png_eps_link_files 
+png_picts: dia_png_files png_png_files
+
+# PNG transformations
+dia_png_files: $(dia_images:%.dia=PNG/%.png)
+png_png_files: $(png_images:%.png=PNG/%.png)
+
+PNG/%.png: %.dia
+	dia --nosplash --export=$@ $< >& /dev/null
+
+PNG/%.png: %.png
+	\rm -f $@
+	ln -s ../$< $@
+
+# EPS transformations
+dia_eps_files: $(dia_images:%.dia=EPS/%.eps)
+dia_eps_link_files: $(dia_images:%.dia=EPS/%.png)
+
+png_eps_files: $(png_images:%.png=EPS/%.eps)
+png_eps_link_files: $(png_images:%.png=EPS/%.png)
+
+EPS/%.eps: %.dia
+	dia --nosplash --export=$@ $< >& /dev/null
+
+EPS/%.png: EPS/%.eps
+	\rm -f $@
+	ln -s `echo $< | sed -e 's/EPS\///'` $@
+
+EPS/%.eps: %.png
+	convert $< $@
+
