+	GdaQueryTarget  *modif_target;
+} QueryExecInstance;
+
+static void
+query_exec_instance_free (QueryExecInstance *qei)
+{
+	/* REM: we need to destroy qei->combo first because it uses a GdaDataModel which
+	 * has GdaValue of type GDA_VALUE_TYPE_GOBJECT which increment the refence count of
+	 * the GdaQueryTarget it references, and which will be actually destroyed by the 
+	 * destruction of the query */
+	if (qei->combo) 
+		gtk_widget_destroy (qei->combo);
+	if (qei->query)
+		g_object_unref (G_OBJECT (qei->query));
+
+	g_free (qei);
+}
+
+static void
+query_exec_entity_to_modify_changed_cb (GnomeDbCombo *combo, QueryExecInstance *qei)
+{
+	GtkWidget *grid;
+	GSList *values;
+	GdaQueryTarget *target = NULL;
+	GdaDataModel *model;
+
+	values = gnome_db_combo_get_values (combo);
+	if (values)
+		target = (GdaQueryTarget *) gda_value_get_gobject (values->next->data);
+
+	if (qei->grid)
+		gtk_widget_destroy (qei->grid);
+
+	qei->modif_target = target;
+	model = gda_data_model_query_new (qei->query);
+	/* TODO: fix the GdaDataModelQuery object to handle qei->modif_target spec */
+	TO_IMPLEMENT;
+	grid = gnome_db_grid_new (model);
+	g_object_unref (model);
+
+	qei->grid = grid;
+
+	gtk_box_pack_start (GTK_BOX (qei->grid_box), grid, TRUE, TRUE, 0);
+	gtk_widget_show (grid);
+	gtk_widget_set_size_request (grid, 800, 300);
+}
+
+static GdaDataModel *
+query_exec_make_entities_model (GdaQuery *query)
+{
+	GdaDataModel *model = NULL;
+	GSList *targets, *list;
+	gchar *str;
+
+	/* REM: we only want targets for which the represented entity id a GdaDictTable and for which we
+	 * have all the PK fields present in the query */
+	targets = gda_query_get_targets (query);
+	list = targets;
+	while (list) {
+		GdaQueryTarget *target = GDA_QUERY_TARGET (list->data);
+		GdaEntity *ent;
+
+		ent = gda_query_target_get_represented_entity (target);
+		if (GDA_IS_DICT_TABLE (ent)) {
+			GSList *pkfields;
+
+			pkfields = gda_query_get_target_pkfields (query, target);
+			if (pkfields) {
+				GList *values = NULL;
+				g_slist_free (pkfields);
+
+				if (!model)
+					model = gda_data_model_array_new (2);
+				
+				str = gda_renderer_render_as_sql (GDA_RENDERER (target), NULL, 0, NULL);
+				values = g_list_append (values, gda_value_new_string (str));
+				g_free (str);
+				
+				values = g_list_append (values, gda_value_new_gobject (G_OBJECT (target))); /* REF added on target! */
+				
+				gda_data_model_append_values (model, values, NULL);
+				g_list_foreach (values, (GFunc) gda_value_free, NULL);
+				g_list_free (values);
+			}
+		}
+		list = g_slist_next (list);
+	}
+	g_slist_free (targets);
+
+	return model;
+}
